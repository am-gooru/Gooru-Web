<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="unitStyle" src="tabContainer.css" />
	<ui:with field="i18n" type="org.ednovo.gooru.shared.i18n.MessageProperties"/>
	<g:HTMLPanel>
		<div class="collection_teacher_tab_container">
				<g:HorizontalPanel styleName="{unitStyle.tabs_teacher_Summary}">
				  <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button ui:field="btnScoredQuestions" styleName="primary"  text="{i18n.GL2282}"></g:Button>  </g:HTMLPanel>
				  <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button ui:field="btnOpenEndedQuestions" styleName="primary" text="{i18n.GL2283}"></g:Button>  </g:HTMLPanel>
				  <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button styleName="primary" ui:field="btnCollectionBreakDown" text="{i18n.GL2284}"></g:Button>  </g:HTMLPanel>
				  <g:HTMLPanel styleName="{unitStyle.printSaveContainer}"> <g:Button styleName="{unitStyle.print_icon_image}" ui:field="btnPtint"></g:Button>  </g:HTMLPanel>
				  <g:HTMLPanel> <g:Button styleName="{unitStyle.mail_icon_image}" ui:field="btnEmail"></g:Button>  </g:HTMLPanel>
			      <g:HTMLPanel> <g:Button styleName="{unitStyle.save_icon_image}" ui:field="btnSave"></g:Button>  </g:HTMLPanel>
				</g:HorizontalPanel>
			</div>
	</g:HTMLPanel>
</ui:UiBinder> 