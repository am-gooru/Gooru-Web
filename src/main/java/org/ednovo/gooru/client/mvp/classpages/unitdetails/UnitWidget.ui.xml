<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<ui:style field="unitStyle" src="unitassignment.css" />
		<!-- Add GWT widgets here. Enter < and hit Ctrl-Space to auto-complete 
			widget names. The ui:field attribute binds an element to a field in the owner 
			class. See the GWT docs on UI Binder for more details. -->
			<g:HTMLPanel>
				<uc:LiPanel ui:field="liPanelUnit">
				      <uc:HTMLEventPanel styleName="{unitStyle.unitPanelStyle}" ui:field="unitNameContainer">
						<g:InlineLabel styleName="{unitStyle.unitPanelnumber}" ui:field="ilUnitNumber"></g:InlineLabel>
						<g:InlineLabel styleName="{unitStyle.unitPanelname}" ui:field="ilUnitName"></g:InlineLabel>
					  </uc:HTMLEventPanel>
				</uc:LiPanel>
				<hr/>
			
			</g:HTMLPanel>
			<!-- <uc:LiPanel styleName="{unitStyle.active}">
			    <g:HTMLPanel styleName="{unitStyle.unitPanelStyle}">
					<g:InlineLabel styleName="{unitStyle.number}">2</g:InlineLabel>
					<g:InlineLabel styleName="{unitStyle.name}">Moon</g:InlineLabel>
				</g:HTMLPanel>	
			</uc:LiPanel> -->
</ui:UiBinder> 