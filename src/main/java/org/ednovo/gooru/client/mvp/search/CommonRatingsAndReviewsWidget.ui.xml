<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="style" src="searchRatingAndReview.css"/>
				  <g:HTMLPanel styleName="{style.totalReviews}" ui:field="totalReviewsPanel">
					    <g:HTMLPanel styleName="{style.leftPanel} {style.borderBottom}" ui:field="totalReviesLeftPnl">
					      <div class="{style.reactionImg}"> <img src="../images/searchAnalyticsImages/emotic-big.png"/> </div>
					      <div class="{style.reactionDescription}"> Average Reaction for <br/>
					       <span> 31</span> Students </div>
					      <div class="{style.clearfix}"></div>
					    </g:HTMLPanel>
					    <g:HTMLPanel styleName="{style.rightPanel} {style.borderBottom} {style.avgReviews}" ui:field="totalReviesRightPnl">
					      <div class="{style.reactionImg}"> <img src="../images/searchAnalyticsImages/ratings.png"/> </div>
					      <div class="{style.reactionDescription}"> Average Reaction for <br/>
					       <span> 31</span> Users  
					       </div>
					       <div class="{style.reactionReview}"><span> 5 </span><br/>
					        Reviews 
					        </div>
					    </g:HTMLPanel>
				        <div class="{style.clearfix}"></div>
				    </g:HTMLPanel>
</ui:UiBinder> 