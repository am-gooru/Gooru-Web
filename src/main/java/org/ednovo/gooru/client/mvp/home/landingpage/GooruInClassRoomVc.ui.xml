<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="landingPageStyle" src="../landing-page.css" type="org.ednovo.gooru.client.mvp.home.LandingPageStyleCss" />
	<g:HTMLPanel styleName="{landingPageStyle.pageSection} {landingPageStyle.whyUseGooru}" ui:field="panelGooruClassRoom">
	   <h2><g:Label ui:field="classroomText"></g:Label></h2>
	   <g:Anchor ui:field="backto" href="#" styleName="{landingPageStyle.returnToTop}"></g:Anchor> 
	   <g:HTMLPanel styleName="{landingPageStyle.contentBlock}">
	      <h3><g:Label ui:field="teachingMethodsLbl"></g:Label></h3>
	      <g:HTMLPanel styleName="{landingPageStyle.methodsList} {landingPageStyle.tabs}">
	         <panel:HTMLEventPanel ui:field="gooruCrTab1" styleName="{landingPageStyle.methodsListLi} {landingPageStyle.methodsLiFirst}"></panel:HTMLEventPanel>
	         <panel:HTMLEventPanel ui:field="gooruCrTab2" styleName="{landingPageStyle.methodsListLi}"></panel:HTMLEventPanel>
	         <panel:HTMLEventPanel ui:field="gooruCrTab3" styleName="{landingPageStyle.methodsListLi}"></panel:HTMLEventPanel>
	         <panel:HTMLEventPanel ui:field="gooruCrTab4" styleName="{landingPageStyle.methodsListLi}"></panel:HTMLEventPanel>
	         <panel:HTMLEventPanel ui:field="gooruCrTab5" styleName="{landingPageStyle.methodsListLi} {landingPageStyle.methodsLiLast}"></panel:HTMLEventPanel>
	      </g:HTMLPanel>
	      <g:HTMLPanel styleName="{landingPageStyle.clearfix} {landingPageStyle.methodsContent}" ui:field="methodsContentContainer">
	      </g:HTMLPanel>
	   </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>