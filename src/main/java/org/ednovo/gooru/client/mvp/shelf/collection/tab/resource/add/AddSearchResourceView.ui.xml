<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc"
	xmlns:qac="urn:import:org.ednovo.gooru.client.mvp.shelf.collection.tab.resource.add" xmlns:tinyMce="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field= "addWebResourceStyle" src="addResource.css" type="org.ednovo.gooru.client.mvp.shelf.collection.tab.resource.add.AddResourceBundle"/>
<g:HTMLPanel styleName="{addWebResourceStyle.addResourceFormOuterDiv}">
   <g:HTMLPanel styleName="{addWebResourceStyle.addResourceFormContainer}">
   
    <g:HTMLPanel ui:field="searchResultspanel">
	  	  <g:Label styleName="{addWebResourceStyle.addSearchTitleText}" ui:field="suggestedText"></g:Label>
	      <g:Label styleName="{addWebResourceStyle.addSearchContentText}" ui:field="totalResources"/>
	      <uc:HTMLEventPanel styleName="{addWebResourceStyle.suggestedResources}" ui:field="suggestedResourcesPanel"/>
	      <g:HTMLPanel ui:field="noResultsPanel">
	      <g:HTML ui:field="noResultsLabel"></g:HTML>
	    </g:HTMLPanel>
   	</g:HTMLPanel>
   
   
   	  <g:Label styleName="{addWebResourceStyle.addSearchTitleText}" ui:field="searchtitleText"></g:Label>
      <g:Label styleName="{addWebResourceStyle.addSearchContentText}" ui:field="serachcontentText"></g:Label>
      <g:HTMLPanel styleName="{addWebResourceStyle.addResourceSearchContent}">
         <g:HTMLPanel styleName="{addWebResourceStyle.addResourceFormInputText}">
            <g:TextBox styleName="{addWebResourceStyle.addResourceFormInputTextBox}" ui:field="searchBox"/>
         </g:HTMLPanel>
         <g:Button styleName="{addWebResourceStyle.addResourceButton}" ui:field="addResourceButton"></g:Button>
      </g:HTMLPanel>
	 
   </g:HTMLPanel>
</g:HTMLPanel>
</ui:UiBinder>