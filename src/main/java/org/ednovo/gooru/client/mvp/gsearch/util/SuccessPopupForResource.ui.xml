<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
	<div id="addCollectionPopup">
		<div class="popHeader">
			<div class="col-md-10 popHeading">Thank you!</div>
				<div class="col-md-2"><g:Anchor ui:field="cancelResourcePopupBtnLbl" styleName="closeButton"></g:Anchor></div>
			<div class="clearfix"></div>
		</div>
	    <g:HTMLPanel styleName="popBody">
	    	<g:Label styleName="lblSuccessImage"></g:Label>
	    	<g:Label ui:field="lblSuccessText" styleName="lblSuccessText"></g:Label>
	    </g:HTMLPanel>
		<div class="clearfix"></div>
		<div class="popFooter">
			<div class="addButton leftAlign"><g:Button styleName="primary" ui:field="btnContinueSearching">Continue Searching</g:Button></div>
			<div class="addButton rightAlign"><g:Button styleName="primary" ui:field="btnViewInMyCollections">View in My Collections</g:Button></div>
		</div>
		<div class="clearfix"></div>
	</div>
</g:HTMLPanel>
</ui:UiBinder> 