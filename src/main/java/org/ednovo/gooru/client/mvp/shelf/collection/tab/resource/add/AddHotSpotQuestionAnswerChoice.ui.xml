	         <g:Label  ui:field="answerHeadingTypeLbl" styleName="htQAnswerHeadings htQAnswerTypeLbl"></g:Label>
	        <g:RadioButton ui:field="imageRDButton" name="ansTypeGroup" styleName="htQAnswerHeadings"></g:RadioButton>
	         <g:RadioButton ui:field="textRDButton" name="ansTypeGroup" styleName="htQAnswerHeadings"></g:RadioButton>
	     </g:HTMLPanel>
	           <g:HTMLPanel styleName="col-md-7 colmdPadding textRight">
	         <g:Label  ui:field="correctLbl" styleName="htQAnswerHeadings"></g:Label>
	         <g:RadioButton ui:field="singleRDButton" name="ansType" styleName="htQAnswerHeadings"></g:RadioButton>
	         <g:RadioButton ui:field="multiRDButton" name="ansType" styleName="htQAnswerHeadings"></g:RadioButton>
	          </g:HTMLPanel>
	          <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
	     </g:FlowPanel>
	<g:HTMLPanel styleName=" htQanswerContainer">
