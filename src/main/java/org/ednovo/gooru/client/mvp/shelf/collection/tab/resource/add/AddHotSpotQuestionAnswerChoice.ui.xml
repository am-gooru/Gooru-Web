	              </g:HTMLPanel>
	         </g:HTMLPanel>
	         
	           <g:HTMLPanel styleName="col-md-10 colmdPadding">
	                 <g:HTMLPanel styleName="imgAnsContainer">
	                     
	                 </g:HTMLPanel>
	           </g:HTMLPanel>
	          <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
	     </g:HTMLPanel>
	     <g:HTMLPanel ui:field="textAnsContainer" styleName="containerBottom"/>
	      <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
	      <g:Anchor styleName="{addWebResourceStyle.addResourceBottmTitle}" ui:field="addAnswerChoice"></g:Anchor>
       </g:HTMLPanel>
       </g:HTMLPanel>
</ui:UiBinder>