<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with
		type="org.ednovo.gooru.client.mvp.classpages.assignments.AddAssignmentContainerCBundle"
		field="res"></ui:with>
	<ui:with field="i18n" type="org.ednovo.gooru.shared.i18n.MessageProperties" />
		<g:HTMLPanel>
	<div id="addCollectionPopup">
		<div class="popHeader">
		<g:Label styleName="col-md-10 popHeading" ui:field="addtocollHeaderText"></g:Label>
		<div class="col-md-2"><g:Anchor ui:field="cancelResourcePopupBtnLbl" styleName="closeButton"></g:Anchor></div>
		<div class="clearfix"></div>
		</div>
		<g:HTMLPanel ui:field="myCollDefault" styleName="foldermenuHeader">
			My Collections (default)
		</g:HTMLPanel>
		<g:ScrollPanel ui:field="dropdownListContainerScrollPanel" styleName="dropdownListContainerScrollPanel">
	          <g:HTMLPanel ui:field="floderTreeContainer" styleName="popBody"/>
		</g:ScrollPanel>
			<div class="clearfix"></div>
			<div class="popFooter">
				<div class="addButton leftAlign"><g:Button styleName="addNewCollectionStyle" ui:field="btnAddNew">+Add New Collection</g:Button></div>
				<div class="addButton rightAlign"><g:Button styleName="primary" ui:field="btnAddExisting" text="{i18n.GL0590}" title="{i18n.GL0590}">Add</g:Button></div>
			</div>
		<div class="clearfix"></div>
	</div>
</g:HTMLPanel>
</ui:UiBinder> 