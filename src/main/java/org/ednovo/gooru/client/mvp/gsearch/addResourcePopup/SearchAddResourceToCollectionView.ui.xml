<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with
		type="org.ednovo.gooru.client.mvp.classpages.assignments.AddAssignmentContainerCBundle"
		field="res"></ui:with>
		<g:HTMLPanel>
	<div id="addCollectionPopup">
		<div class="popHeader">
		<div class="col-md-10 popHeading">Add this collection to one of your existing or new collection</div>
		<div class="col-md-2"><g:Anchor ui:field="cancelResourcePopupBtnLbl" styleName="closeButton"></g:Anchor></div>
		<div class="clearfix"></div>
		</div>
		<g:ScrollPanel ui:field="dropdownListContainerScrollPanel" styleName="dropdownListContainerScrollPanel">
	          <g:HTMLPanel ui:field="floderTreeContainer" styleName="popBody"/>
		</g:ScrollPanel>
			<div class="clearfix"></div>
			<div class="popFooter">
				<div class="addButton leftAlign"><g:Button styleName="primary" ui:field="btnAddNew">Add to New</g:Button></div>
				<div class="addButton rightAlign"><g:Button styleName="primary" ui:field="btnAddExisting">Add to Existing</g:Button></div>
			</div>
		<div class="clearfix"></div>
	</div>
</g:HTMLPanel>
</ui:UiBinder> 