<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc"
xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style type="org.ednovo.gooru.client.mvp.search.SearchFilterVc.Style">

		.searchFilterVcContainer {
			float: left;
			clear: both;
			min-height: 450px;
			width: 185px;
			padding-left: 14px;
			margin-right: 10px;
			overflow-x: hidden;
			background-color: #FFFFFF;
			box-sizing: border-box;
			-moz-box-sizing: border-box;
		}
		
		.active {
			border-bottom: 2px solid #4E9746 !important;
			font-weight: bold;
		}
		
		.titleText {
			padding-bottom: 2px;
			margin-top: 20px;
			border-bottom: 1px #DDD solid;
			font-size: 16px !important;
		}
		
		.notFound {
			clear: both;
			float: left;
			color: orange;
			display:none;
		}
		
		.clearAll {
			color: #4e9746;
			float: right;
			width: 50px;
			cursor: pointer;
			margin-top: -19px;
		}
		.clearAll:HOVER {
			color: #A6CAA2;
		}

		.helpIconBtn {
			position: absolute;
			right: 0;
			margin-top: -20px;
			margin-right: 10px;
			color: #0f76bb;
			cursor: pointer;
			font-size: 11px;
		}
		.helpIconBtnAggregator {
			position: absolute;
			right: 0;
			margin-top: -20px;
			margin-right: 65px;
			color: #0f76bb;
			cursor: pointer;
			font-size: 11px;
			background: url(images/mos/questionmark.png) no-repeat;
			width: 18px;
			height: 15px;
		}
		.helpIconBtnPublisher {
			position: absolute;
			right: 0;
			margin-top: -20px;
			margin-right: 85px;
			color: #0f76bb;
			cursor: pointer;
			font-size: 11px;
			background: url(images/mos/questionmark.png) no-repeat;
			width: 18px;
			height: 15px;
		}
		.sourceToolTip {
			position: absolute;
			margin-top: -10px;
			width: 140px;
		}
		
		.notifyLink {
			color: #4E9746;
			cursor: pointer;
			float: left;
			margin: 0 2px;
			padding-bottom: 2px;
		}
		.notifyLink:HOVER {
		    border-bottom: 2px solid #A6CAA2;
		    cursor: pointer;
		    padding-bottom: 2px;
		    color: #4E9746;
		    text-decoration:none;
		}
		.notifyLinks {
		    color: #4E9746 !important;
		    cursor: default;
		    float: left;
		    font-size: 15px;
		    font-weight: bold;
		    margin: -3px 5px 0 6px;
		}
		
		.PageNotify {
			margin: 17px auto 0;
			width: 596px;
			display: inline-block;
			vertical-align:top;
		}
		.oer {
			margin: 20px auto 0;
		}
		.aggregatorTooltip{
			left: 82px;
			top: 390px;
			margin-left: 0px;
			position: absolute;
			cursor: pointer;
		}
		.publisherTooltip{
			left: 73px;
			top: 461px;
			margin-left: 0px;
			position: absolute;
			cursor: pointer;
		}

		.headerLbl{
			font-weight: bold;
			display: inline-block;
			margin-bottom: 10px;
		}
		.browseContainer{
			float: left;
			margin-left: -7px;
			margin-top: 2px;

		}
		.standardsBrowseBtn{
			padding: 4px 4px;

		}

		.flowPanelContainer{
			display: inline-block;
		}
		
		.arrowLable{
            background: url(images/ratings/down-arrow.png) 0px 12px no-repeat;
			left: 6px;
			width: 10px;
			height: 17px;
			cursor: pointer;
			margin: 4px 0 0px 0;
			display: inline-block;
       }
       .arrowLableTransform{
    -ms-transform: rotate(270deg); /* IE 9 */
    -webkit-transform: rotate(270deg); /* Chrome, Safari, Opera */
    transform: rotate(270deg);
       background: url(images/ratings/down-arrow.png) 0px 12px no-repeat;
			left: 0px;
			width: 10px;
			height: 17px;
			cursor: pointer;
		margin: 4px 7px 0 -6px;
		display: inline-block;
		top: 2px;
		position: relative;
	}
	.grade1Contianer{
	display: inline-block;
	width: 44%;
	position: relative;
	}
	.grade2Contianer{
	display: inline-block;
	width: 50%;
	position: relative;
	top: -20px;
	left: -23px;

	}
	.reviewCheckBoxStyle{
	vertical-align: middle;
	width: 14px;

	}
	.reviewLabelForCheckbox{
	display: inline-block;
	margin-left: 1px;
	width: 120px;
	vertical-align: top;

	}
	.gradesFilContainer{
	width:100%;

	}

	</ui:style>
	<g:FlowPanel styleName="{style.searchFilterVcContainer}" ui:field="myCollectionSearch">
		<g:FlowPanel>
			<g:Label styleName="{style.titleText}" ui:field="filtersText"/>

			<g:Label  styleName="{style.clearAll}" ui:field="clearAll" />

		</g:FlowPanel>
		
		<!-- <g:FlowPanel styleName="{style.PageNotify}" ui:field="flowpanel">
			<g:HTMLPanel ui:field="contentpanel"></g:HTMLPanel>
			<g:Anchor  styleName="{style.notifyLink}" ui:field="resourceLinkLbl" />
			<g:Label  styleName="{style.notifyLinks}" ui:field="notifyText"/>
			<g:Anchor  styleName="{style.notifyLink}" ui:field="collectionLinkLbl" />
		</g:FlowPanel> -->
		
		
		<g:HTMLPanel ui:field="oerPanel"/>
		
		<g:HTMLPanel ui:field="panelNotMobileFriendly"></g:HTMLPanel>
		
		<g:HTMLPanel>
		<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblCategory" visible="true"></g:Label>
			<g:Label styleName="{style.headerLbl}" ui:field="resourceFormatLbl"></g:Label>
			
			<g:HTMLPanel  ui:field="categoryPanelUc" />
		</g:HTMLPanel>
		
		<g:HTMLPanel>
		<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblSubject" visible="true"></g:Label>
			<g:Label styleName="{style.headerLbl}" ui:field="subjectLbl"></g:Label>
				
			<g:HTMLPanel  ui:field="subjectPanelUc" />
		</g:HTMLPanel>
		
		<g:HTMLPanel>
		<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblGrade" visible="true"></g:Label>
			<g:Label styleName="{style.headerLbl}" ui:field="gradeLbl"></g:Label>
			
			<g:HTMLPanel styleName="{style.gradesFilContainer}">
			<g:HTMLPanel  ui:field="gradePanelUc" styleName="{style.grade1Contianer}"/>
			<g:HTMLPanel  ui:field="gradePanelUcNext" styleName="{style.grade2Contianer}"/>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
		<g:HTMLPanel>
		<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblstandard" visible="true"></g:Label>
		    <g:Label styleName="{style.headerLbl}" ui:field="standardLbl"></g:Label>
		    	
			<g:HTMLPanel  ui:field="standardPanelUc">
			    <g:HTMLPanel styleName="{style.browseContainer}">
					<g:Button ui:field="browseStandards" styleName="secondary">Browse</g:Button>
				</g:HTMLPanel>
				<g:HTMLPanel>
					<uc:AppSuggestBox ui:field="standardSgstBox" width="105px" />
				</g:HTMLPanel>
				<g:HTMLPanel>
				 	<g:Label styleName="{style.helpIconBtn}" ui:field="standardHelpicon" visible="false"/>
				</g:HTMLPanel>
				<g:HTMLPanel>
				 	<panel:HTMLEventPanel ui:field="standardToolTip" visible="false" styleName="{style.sourceToolTip}"/>
				</g:HTMLPanel>
				<g:HTMLPanel>
					<g:Label styleName="{style.notFound}" ui:field="standardsNotFoundLbl" />
				</g:HTMLPanel>
				<g:HTMLPanel>
					<g:FlowPanel styleName="{style.flowPanelContainer}" ui:field="standardContainerFloPanel" />
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
		<g:HTMLPanel>
		<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblratings" visible="true"></g:Label>
			<g:Label styleName="{style.headerLbl}" visible="false" ui:field="ratingsLbl"></g:Label>
				
			<g:HTMLPanel  ui:field="ratingPanelUc" />
			<g:HTMLPanel  ui:field="reviewPanelUc" />
		</g:HTMLPanel>
		
		<g:HTMLPanel>
			<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblsource" visible="true"></g:Label>
		    <g:Label styleName="{style.headerLbl}" visible="false" ui:field="sourceLbl"></g:Label>

			<g:HTMLPanel  ui:field="sourcePanelUc" visible="false">
				<g:HTMLPanel>
				 	<g:Label styleName="{style.helpIconBtnPublisher}" ui:field="publisherTooltip"/>
				</g:HTMLPanel> 
				<g:HTMLPanel>
					<uc:AppSuggestBox ui:field="sourceSgstBox" width="130px" />
				</g:HTMLPanel>
				<!-- <uc:add>
				 	<g:Label styleName="{style.helpIconBtn}" ui:field="sourceHelpicon" visible="false"/>
				</uc:add> -->
				<g:HTMLPanel>
				 	<panel:HTMLEventPanel ui:field="sourceToolTip" visible="false" styleName="{style.sourceToolTip}"/>
				</g:HTMLPanel>
				<g:HTMLPanel>
					<g:Label styleName="{style.notFound}" ui:field="sourcesNotFoundLbl"/>
				</g:HTMLPanel>
				<g:HTMLPanel>
					<g:FlowPanel styleName="{style.flowPanelContainer}" ui:field="sourceContainerFloPanel" />
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
		<g:HTMLPanel>
			<g:HTMLPanel  ui:field="aggregatorPanelUc" visible="false">
				<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblaggregator" visible="true"></g:Label>
			     <g:Label styleName="{style.headerLbl}" visible="false" ui:field="aggregatorLbl"></g:Label>
			    <g:HTMLPanel ui:field="aggregatorPanel">
			 	<g:HTMLPanel>
				 	<g:Label styleName="{style.helpIconBtnAggregator}" ui:field="aggregatorTooltip"/>
				</g:HTMLPanel>
				<g:HTMLPanel>
					<uc:AppSuggestBox ui:field="aggregatorSgstBox" width="130px" />
				</g:HTMLPanel>
				<!-- <uc:add>
				 	<g:Label styleName="{style.helpIconBtn}" ui:field="aggregatorHelpicon" visible="false"/>
				</uc:add> -->
				<g:HTMLPanel>
				 	<panel:HTMLEventPanel ui:field="aggregatorToolTip" visible="false" styleName="{style.sourceToolTip}"/>
				</g:HTMLPanel>
				<g:HTMLPanel>
					<g:Label styleName="{style.notFound}" ui:field="aggregatorNotFoundLbl"/>
				</g:HTMLPanel>
				<g:HTMLPanel>
					<g:FlowPanel styleName="{style.flowPanelContainer}"  ui:field="aggregatorContainerFloPanel" />
				</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
		
		
		<g:HTMLPanel>
		<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblauthor" visible="false"></g:Label>
		    <g:Label styleName="{style.headerLbl}" visible="false" ui:field="authorLbl"></g:Label>		  
			
			<g:HTMLPanel  ui:field="authorPanelUc" visible="false">
				<g:HTMLPanel>
					<g:TextBox ui:field="authorTxtBox" width="130px" />
				</g:HTMLPanel>
				<g:HTMLPanel>
					<g:FlowPanel styleName="{style.flowPanelContainer}" ui:field="authorContainerFloPanel" />
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
		
		<g:HTMLPanel>
		<g:Label styleName="{style.arrowLableTransform}" ui:field="arrowLblaccess" visible="true"></g:Label>
			<g:Label styleName="{style.headerLbl}" visible="false"  ui:field="accessModeLbl"></g:Label>
			   
			<g:HTMLPanel  ui:field="accessModePanel" />
		</g:HTMLPanel>
		
	
		
	</g:FlowPanel>
</ui:UiBinder> 