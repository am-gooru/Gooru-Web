<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.classpages.newclasspage.NewClasspagePopupCBundle"
		field="res" />
		<g:HTMLPanel styleName="{res.css.popupOuter}">
			<g:HTMLPanel styleName="{res.css.popupInner}">
				<g:HTMLPanel styleName="{res.css.popupHeader}" ui:field="headerPanel" />
				<g:HTMLPanel styleName="{res.css.popupContent}">
					<g:HTMLPanel styleName="{res.css.popupTitle}" ui:field="titlePanel">
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{res.css.popupInputBoxContainer}">
						<g:TextBox styleName="{res.css.popupInputBox}" ui:field="classpageTitleTxt" />
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{res.css.popupMandatoryContainer}">
						<g:Label styleName="{res.css.popupMandatoryTitleText}"
							ui:field="mandatoryClasspageTitleLbl"></g:Label>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:Label styleName="{res.css.creating}" ui:field="panelLoading"></g:Label>
				<g:HTMLPanel styleName="{res.css.popupButtinContainer}" ui:field="panelControls">
					<g:Button ui:field="btnCancel" styleName="secondary"/>
					<g:Button ui:field="btnAdd" styleName="primary"/>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
</ui:UiBinder>