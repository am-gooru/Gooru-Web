<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="folderMetaStyle" src="FolderItemMetaDataUc.css" type="org.ednovo.gooru.client.mvp.shelf.collection.folders.FolderItemMetaDataUcStyleBundle" />
	<g:HTMLPanel styleName="{folderMetaStyle.mainSection} {folderMetaStyle.contentBlock}">
	   <g:HTMLPanel styleName="{folderMetaStyle.ideasBlock}" ui:field="bigIdeasPanel">
	      <g:HTMLPanel styleName="{folderMetaStyle.blockView} {folderMetaStyle.idea}">
	         <g:Label styleName="{folderMetaStyle.blockViewTitle}" ui:field="ideasStaticLbl"/>
	      </g:HTMLPanel>
	      <g:HTML styleName="{folderMetaStyle.padding10}" ui:field="bigIdeasLbl"/>
	      <g:TextArea styleName="{folderMetaStyle.textAreaStyle}" ui:field="bigIdeasHTML"/>
	      <g:Label styleName="{folderMetaStyle.errorLabel}" ui:field="charLimitLblForBigIdeas"></g:Label>
	      <g:Label styleName="errorMessage {folderMetaStyle.errorLabel}" ui:field="errorLabelbigIdeasHTML"></g:Label>
	   </g:HTMLPanel>
	   <g:HTMLPanel styleName="{folderMetaStyle.ideasBlock}" ui:field="essentialQuestionsPanel">
	      <g:HTMLPanel styleName="{folderMetaStyle.blockView} {folderMetaStyle.question}">
	         <g:Label styleName="{folderMetaStyle.blockViewTitle}" ui:field="questionsStaticLbl"/>
	      </g:HTMLPanel>
	      <g:HTML styleName="{folderMetaStyle.padding10}" ui:field="essentialQuestionsLbl"/>
	      <g:TextArea styleName="{folderMetaStyle.textAreaStyle}" ui:field="essentialQuestionsHTML"/>
	       <g:Label styleName="{folderMetaStyle.errorLabel}" ui:field="charLimitLblForEssential"></g:Label>
	       <g:Label styleName="errorMessage {folderMetaStyle.errorLabel}" ui:field="errorLabelessentialQuestionsHTML"></g:Label>
	   </g:HTMLPanel>
	   <g:HTMLPanel styleName="{folderMetaStyle.ideasBlock} {folderMetaStyle.ideasBlockLast}" ui:field="performanceTaskPanel">
	      <g:HTMLPanel styleName="{folderMetaStyle.blockView} {folderMetaStyle.task}">
	         <g:Label styleName="{folderMetaStyle.blockViewTitle}" ui:field="tasksStaticLbl"/>
	      </g:HTMLPanel>
	      <g:HTML styleName="{folderMetaStyle.padding10}" ui:field="performanceTaskLbl"/>
	      <g:TextArea styleName="{folderMetaStyle.textAreaStyle}" ui:field="performanceTaskHTML"/>
	       <g:Label styleName="{folderMetaStyle.errorLabel}" ui:field="charLimitLblForPerformance"></g:Label>
	      <g:Label styleName="errorMessage {folderMetaStyle.errorLabel}" ui:field="errorLabelperformanceTaskHTML"></g:Label>
	   </g:HTMLPanel>
	  <g:Button styleName="{folderMetaStyle.closeItem}" ui:field="closeItem"/>
	  <g:HTMLPanel styleName="{folderMetaStyle.formButtons}" ui:field="formButtons">
          <g:Button styleName="primary" ui:field="saveBtn"></g:Button>
      	  <g:Button styleName="secondary" ui:field="cancelBtn"></g:Button>
      </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>