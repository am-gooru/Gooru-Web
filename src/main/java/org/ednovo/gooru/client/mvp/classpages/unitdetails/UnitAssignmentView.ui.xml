<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="unitStyle" src="unitassignment.css" />
	<ui:with field="i18n" type="org.ednovo.gooru.shared.i18n.MessageProperties"/>
	<g:HTMLPanel styleName="{unitStyle.unitDetails}">
		<div class="{unitStyle.containerTeacher}">
			<div class="col-xs-2 {unitStyle.leftPanel}">
				<g:HTMLPanel styleName="{unitStyle.uniSetup}">
					<img src="images/blueArrow.png" />
					<g:Anchor ui:field="unitSetupButton"></g:Anchor>
				</g:HTMLPanel>
				<hr />
				<uc:UlPanel styleName="list-unstyled {unitStyle.unitMenu}">
					<g:HTMLPanel ui:field="unitPanel"></g:HTMLPanel>
				</uc:UlPanel>
				<g:HTMLPanel styleName="{unitStyle.uniSetup}">
					<g:Label styleName="{unitStyle.moreUnits}" ui:field="lblMoreUnits"></g:Label>
				</g:HTMLPanel>
			</div>
			<div class="col-xs-10 {unitStyle.rightPanel}">
				<g:HTMLPanel styleName="{unitStyle.teachTab}" ui:field="htmDashBoardTabs">
					<g:Button ui:field="btnDashBoard"></g:Button>
					<g:Button ui:field="btnAssignment"></g:Button>
				</g:HTMLPanel>
				<g:HTMLPanel ui:field="containerPanel">
				 <div class="row">
						<div class="col-xs-12">
							<g:HTMLPanel styleName="row {unitStyle.classOptionsMain}">
									<g:HTMLPanel styleName="col-xs-2 {unitStyle.smallerWidth} {unitStyle.assignOverOptions} text-center">
										<g:InlineLabel styleName="{unitStyle.bubbleSmall}"></g:InlineLabel> 
										<g:InlineLabel text="{i18n.GL2209}"></g:InlineLabel> 
									</g:HTMLPanel>
									<g:HTMLPanel styleName="col-xs-2 {unitStyle.smallerWidth} {unitStyle.classOverOptions1} text-center">
										<g:InlineLabel styleName="{unitStyle.bubbleSmall} {unitStyle.stylishBub}"></g:InlineLabel>
										<g:InlineLabel text="{i18n.GL2201}"></g:InlineLabel> 
									</g:HTMLPanel>
									
									<g:HTMLPanel ui:field="personalizePanel">
						 				<g:HTMLPanel styleName="col-xs-2 {unitStyle.smallerWidth} {unitStyle.classOverOptions1} text-center">
											<g:InlineLabel styleName="{unitStyle.bubbleSmall} {unitStyle.greenBubble}"></g:InlineLabel>
											<g:InlineLabel text="{i18n.GL2239}"></g:InlineLabel> 
										</g:HTMLPanel>
										<g:HTMLPanel styleName="col-xs-2 {unitStyle.smallWidthGoalNotMet} {unitStyle.classOverOptions1} text-center">
											<g:InlineLabel styleName="{unitStyle.bubbleSmall} {unitStyle.redBubble}"></g:InlineLabel> 
											<g:InlineLabel text="{i18n.GL2240}"></g:InlineLabel>
										</g:HTMLPanel>
										<g:HTMLPanel styleName="col-xs-2 {unitStyle.smallWidth} {unitStyle.classOverOptions} text-center">
											<g:InlineLabel styleName="{unitStyle.bubbleSmallrounded}"></g:InlineLabel> 
											<g:InlineLabel text="{i18n.GL2212}"></g:InlineLabel>
										</g:HTMLPanel>
										<g:HTMLPanel styleName="col-xs-2 {unitStyle.smallWidthFDS} {unitStyle.classOverOptionsFeedback} text-center">
										    <g:InlineLabel styleName="{unitStyle.bubbleBlueFeedBack}"></g:InlineLabel> 
										   	<g:InlineLabel text="{i18n.GL2238}"></g:InlineLabel>
										</g:HTMLPanel> 
									</g:HTMLPanel>
									
							</g:HTMLPanel>
							<g:Label ui:field="unitTitleDetails" styleName="{unitStyle.unitHeading}" />
							<div class="clearfix"></div>
						</div>
					</div> 
					<!-- This widget Is used to display Circle -->
					 <g:HTMLPanel styleName="row {unitStyle.generalSet}">
						<g:HTMLPanel>

							<g:HTMLPanel styleName="{unitStyle.rowInner}"
								ui:field="circleContainerPanel">
							</g:HTMLPanel>
						</g:HTMLPanel>
					</g:HTMLPanel>
		
					<!-- end circle Widget -->
						<g:HTMLPanel ui:field="assignmentWidgetConatiner"/>
					<g:HTMLPanel styleName="row {unitStyle.personalBtn}" ui:field="panelPersonalizeButtonContainer">
						<div class="col-xs-12">
							<div class="{unitStyle.rowInner}">
								<p class="text-center">
									<g:Button styleName="primary" ui:field="btnPersonalize"></g:Button>
								</p>
							</div>
						</div>
					</g:HTMLPanel>
					<g:HTMLPanel ui:field="assignmentContainer"></g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel ui:field="goalContainer">

					<div class="row sectionBlockMain">
						<div class="clearfix"></div>

						<div class="col-xs-12 goalMain">
							<div class="col-xs-6 mdiumHeader">Goals</div>
						</div>
						<div class="clearfix"></div>

						<div class="col-xs-12 scoreProggressMain">
							<div class="">
								<g:HTMLPanel ui:field="scoreHedingContainer"></g:HTMLPanel>
								   <div class="col-xs-4"> <div class="scorMetInner"> <div class="scoreHeding">Time Studing</div> 
										<div class="goalTracker"> <div class="circle" id="tracker"> 
										<g:Label styleName="{unitStyle.circle}" ui:field="lblGreenControl" ></g:Label>
									    <g:Label styleName="{unitStyle.circle}" ui:field="lblControl"></g:Label> 
										<div class="circle" id="circleMiddle"></div>
									    <div class="emptybox"></div> 
										</div> 
										<div class="setGloalBtn"> <div class="timeTextBox">
										    <g:TextBox ui:field="txtHours" />
										    <g:TextBox ui:field="txtMinuts" />
										</div> 
										<g:Label ui:field="lblTimeValidation"></g:Label>
										<g:HTMLPanel ui:field="timeLablePanel">
										   <g:Label styleName="{unitStyle.timeLabelStyle}" ui:field="lblTimeHours"></g:Label> 
										   <g:Label styleName="{unitStyle.timeLabelStyle}" ui:field="lblTimeMin"></g:Label> 
										</g:HTMLPanel>
										
										<p><g:Button  styleName="primary" ui:field="btnSetGoal"></g:Button></p> </div> </div> </div>

								    </div> 



							</div>
						</div>
					</div>
					<div class="clearfix"></div>
				</g:HTMLPanel>
				<g:HTMLPanel ui:field="personalizeMainContainer">
				    <g:HTMLPanel ui:field="personalizeContainer"></g:HTMLPanel>
				</g:HTMLPanel>
			</div>
			<div class="clearfix"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>