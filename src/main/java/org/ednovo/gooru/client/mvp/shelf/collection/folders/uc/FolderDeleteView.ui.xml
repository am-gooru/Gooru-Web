<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="FolderStyle" src="folderPopup.css"/>
	    <g:HTMLPanel styleName="{FolderStyle.popup}">
			 <g:HTMLPanel styleName="{FolderStyle.popupInner}">
			    <g:Label styleName="{FolderStyle.popupHeader}" ui:field="titleLabel"/>
				 <g:HTMLPanel styleName="{FolderStyle.popupContent}">
                    <g:HTMLPanel styleName="{FolderStyle.icon} {FolderStyle.delete}"/>
                     <g:Label styleName="{FolderStyle.header}" ui:field="headerLabel"/>
                      <g:Label styleName="{FolderStyle.descriptionText}" ui:field="descriptionLabel"/>
                      <g:Label styleName="{FolderStyle.deleteTextLable}" ui:field="lblDeleteText"/>
                     	<gg:TextBoxWithPlaceholder styleName="{FolderStyle.inputText}"
								ui:field="txtDelete" />
                 </g:HTMLPanel>
				 <g:HTMLPanel styleName="{FolderStyle.okCancel}">
				    <g:Label ui:field="deleteText" styleName="{FolderStyle.deleteText}"/>
				    <g:Button styleName="secondary" ui:field="cancelButton"/>
				     <g:Button styleName="primary {FolderStyle.buttonMargins}" ui:field="okButton"/>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
</ui:UiBinder> 