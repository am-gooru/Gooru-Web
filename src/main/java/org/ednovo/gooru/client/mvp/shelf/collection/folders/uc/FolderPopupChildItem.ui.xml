<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="folderPopupStyle" src="folderPopup.css" type="org.ednovo.gooru.client.mvp.shelf.collection.folders.uc.FolderPopupStyleBundle" />
	<gg:HTMLEventPanel styleName="{folderPopupStyle.folderLevel}" ui:field="folderLevel">
	    <g:HTMLPanel styleName="{folderPopupStyle.arrow}" ui:field="arrow"></g:HTMLPanel>
		<g:Label styleName="{folderPopupStyle.title}" ui:field="title"></g:Label>
	</gg:HTMLEventPanel>
</ui:UiBinder>