<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="landingPageStyle" src="../landing-page.css"  type="org.ednovo.gooru.client.mvp.home.LandingPageStyleCss"/>
		<g:HTMLPanel styleName="{landingPageStyle.pageSection} featuredCollections" ui:field="panelTeachersPick">
		   <g:HTMLPanel ui:field="teachersPickPanel" styleName="{landingPageStyle.teachersPickHeading}"/>
		   <g:HTMLPanel styleName="{landingPageStyle.courseTabs}">
		      <panel:HTMLEventPanel ui:field="mathTab" styleName="{landingPageStyle.courseTabsLi}"></panel:HTMLEventPanel>
		      <panel:HTMLEventPanel ui:field="scienceTab" styleName="{landingPageStyle.courseTabsLi}"></panel:HTMLEventPanel>
		      <panel:HTMLEventPanel ui:field="socialTab" styleName="{landingPageStyle.courseTabsLi}"></panel:HTMLEventPanel>
		      <panel:HTMLEventPanel ui:field="languageTab" styleName="{landingPageStyle.courseTabsLi}"></panel:HTMLEventPanel>
		      <panel:HTMLEventPanel ui:field="featuredTab" styleName="{landingPageStyle.courseTabsLi}"></panel:HTMLEventPanel>
		   </g:HTMLPanel>
		   <g:HTMLPanel styleName="{landingPageStyle.featuredContent}" ui:field="featuredContainer"/>
		</g:HTMLPanel>
</ui:UiBinder> 