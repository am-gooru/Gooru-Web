<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc"
	xmlns:dt="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style field="style" src="collections.css" />
	<g:FlowPanel styleName="{style.classpageHeader}">
	    <g:FlowPanel styleName="{style.headerLeft}">
	     	<g:HTMLPanel styleName="{style.headerText}" ui:field="dueDateText"></g:HTMLPanel>
	    	<g:HTMLPanel styleName="{style.headerDatePicker}" ui:field="dueDatePanel"></g:HTMLPanel>
	      <g:Button styleName="secondary" ui:field="cancelButton"></g:Button>
	      <g:Button styleName="primary {style.cancelButton}" ui:field="saveButton"></g:Button>
	      <g:Label styleName="{style.savingText}" ui:field="savingText"></g:Label>
	    </g:FlowPanel>
	 </g:FlowPanel>
</ui:UiBinder> 