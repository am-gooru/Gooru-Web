<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="flagPopUpstyle" src="player-flag-popup.css"/>
	<g:HTMLPanel>
		<div class="{flagPopUpstyle.thankyoupopupwrapper}">
	<div class="{flagPopUpstyle.thankyoupopupinnerwrapper}">
    	<div class="{flagPopUpstyle.playerflagpopupheader}">
        	<g:Label styleName="{flagPopUpstyle.playerflagpopupheadertitle}" ui:field="successPopupTitle"/>
            <panel:HTMLEventPanel ui:field="closeButton" styleName="{flagPopUpstyle.playerflagpopupheaderCloseBtn}">
        	    <g:Image ui:field="popUpCloseButton"/>
        	</panel:HTMLEventPanel> 
        </div>
        <div class="{flagPopUpstyle.playerflagcontentcontainerThankYou}">
        	<g:Label styleName="{flagPopUpstyle.playerflagcontentthankyou}" ui:field="successPopupMessage"/>
        	<g:HTMLPanel ui:field="emailPopupThanksmsg">
        	     <g:Label styleName="{flagPopUpstyle.emailThankYouLbl}" ui:field="emailConfirmationText"></g:Label>
        	      <g:Label styleName="{flagPopUpstyle.emailIdLbl}" ui:field="emailIdLbl"></g:Label>
        	 </g:HTMLPanel>
            <g:Button styleName="{flagPopUpstyle.playerflagbuttonOk}" ui:field="okButton"/> 
           </div>
</div>
</div>
	</g:HTMLPanel>
</ui:UiBinder> 